<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>hospital_reg</servlet-name>
        <servlet-class>hospital_reg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>hospital_serv</servlet-name>
        <servlet-class>hospital.hospital_serv</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>hosplog_serv</servlet-name>
        <servlet-class>hospital.hosplog_serv</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>admin</servlet-name>
        <servlet-class>hospital.admin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>dr_reg</servlet-name>
        <servlet-class>hospital.dr_reg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>doc_login</servlet-name>
        <servlet-class>hospital.doc_login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>patientPres</servlet-name>
        <servlet-class>hospital.patientPres</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>hospital_reg</servlet-name>
        <url-pattern>/hospital_reg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>hospital_serv</servlet-name>
        <url-pattern>/hospital_serv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>hosplog_serv</servlet-name>
        <url-pattern>/hosplog_serv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>admin</servlet-name>
        <url-pattern>/admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>dr_reg</servlet-name>
        <url-pattern>/dr_reg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>doc_login</servlet-name>
        <url-pattern>/doc_login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>patientPres</servlet-name>
        <url-pattern>/patientPres</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
